{
    "author": "kai zhu <kaizhu256@gmail.com>",
    "description": "#### basic api documentation for  [hostile (v1.3.0)](https://github.com/feross/hostile)  [![npm package](https://img.shields.io/npm/v/npmdoc-hostile.svg?style=flat-square)](https://www.npmjs.org/package/npmdoc-hostile) [![travis-ci.org build-status](https://api.travis-ci.org/npmdoc/node-npmdoc-hostile.svg)](https://travis-ci.org/npmdoc/node-npmdoc-hostile)",
    "devDependencies": {
        "electron-lite": "kaizhu256/node-electron-lite#alpha",
        "utility2": "kaizhu256/node-utility2#alpha"
    },
    "engines": {
        "node": ">=4.0"
    },
    "homepage": "https://github.com/npmdoc/node-npmdoc-hostile",
    "keywords": [
        "documentation",
        "hostile"
    ],
    "license": "MIT",
    "main": "lib.npmdoc_hostile.js",
    "name": "npmdoc-hostile",
    "nameAlias": "npmdoc_hostile",
    "nameOriginal": "npmdoc-hostile",
    "os": [
        "darwin",
        "linux"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/npmdoc/node-npmdoc-hostile.git"
    },
    "scripts": {
        "build-ci": "utility2 shReadmeTest build_ci.sh",
        "env": "env",
        "heroku-postbuild": "npm install \"kaizhu256/node-utility2#alpha\" && utility2 shDeployHeroku",
        "postinstall": "[ ! -f npm_scripts.sh ] || ./npm_scripts.sh postinstall",
        "start": "PORT=${PORT:-8080} utility2 start test.js",
        "test": "(set -e; export PORT=$(utility2 shServerPortRandom); utility2 test test.js)"
    },
    "version": "2017.4.25"
}
